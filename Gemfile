gem "rexml", "3.4.1"
gem "concurrent-ruby", "1.3.5"
gem "logger", "1.6.6"
gem "faraday", "2.12.2"
gem "execjs", "2.10.0"
gem "activesupport", "7.1.5.1"
gem "faraday-net_http", "3.4.0"
